#include<stdio.h>
#include<stdlib.h>
#define SIZE 100
int tree[SIZE];
void init(){
    for(int i=0; i<SIZE; i++){
        tree[i] = -1;
    }
}
void insert(int n){   // Level order insertion in binary tree
    for(int i=0; i<n; i++){
        printf("Enter the value of Node : ");
        scanf("%d", &tree[i]);
    }
}
void preorder(int root){
    if(tree[root] == -1)
        return;
    printf("%d ", tree[root]);
    preorder(2*root + 1); // Left child
    preorder(2*root + 2); // Right child
}
void inorder(int root){
    if(tree[root] == -1)
        return;
    inorder(2*root + 1); // Left child
    printf("%d ", tree[root]);
    inorder(2*root + 2); // Right child
}
void postorder(int root){
    if(tree[root] == -1)
        return;
    postorder(2*root + 1); // Left child
    postorder(2*root + 2); // Right child
    printf("%d ", tree[root]);
}
int main(){ // 10 20 30 40 -1 60 70 80 90 -1 -1 -1 -1 100
   int n;
   printf("Enter the value of N : ");
   scanf("%d", &n);
   init();
   insert(n);
   printf("\nPre-order : ");
   preorder(0);
   printf("\nIn-Oreder : ");
   inoreder(0);
   printf("\nPost-Order : ");
   postorder(0);
   return 0;
}
